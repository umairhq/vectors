#include <iostream>
#include <vector>
using namespace std;

int main(){
	vector<int> v(6);

	for(int i = 0; i < v.size(); i++){
		cin >> v[i];
	}

	int x;
	cin >> x;

	int slarge = 0;

	for(int i = 0; i < v.size(); i++){
		if(v[i] > x){ // large of x element
			slarge++;
		}
	}

	cout << slarge;

}
